{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Launch Violentmonkey in FF",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "addonPath": "${workspaceFolder}/dist",
      "pathMappings": [
        {
          "url": "webpack:///src/common",
          "path": "${workspaceFolder}/src/common"
        }
      ]
    }
  ]
}
